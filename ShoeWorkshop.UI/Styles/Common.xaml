<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--#region Colors-->
    <Color x:Key="MainNavigationFontColor">#FF4F30</Color>
    <Color x:Key="MainNavigationColor">#262627</Color>
    <Color x:Key="ActiveMainNavigationColor">#404040</Color>
    <!--#endregion-->
    
    <!--#region Brushes-->
    <SolidColorBrush x:Key="MainNavigationFontColorBrush" Color="{StaticResource MainNavigationFontColor}"></SolidColorBrush>
    <SolidColorBrush x:Key="MainNavigationBrush" Color="{StaticResource MainNavigationColor}"></SolidColorBrush>
    <SolidColorBrush x:Key="MainUnactiveNavigationBrush" Color="{StaticResource ActiveMainNavigationColor}"></SolidColorBrush>
    <!--#endregion-->
    
    <!--#region Font-->
    <FontFamily x:Key="NavigationFontFamily">Comic Sans MS</FontFamily>
    <FontWeight x:Key="NavigationFontWeight">DemiBold</FontWeight>
    <!--#endregion-->
</ResourceDictionary>